[core]
  editor = nvim
[user]
	name = Kevin
	email = kevin.j.mcnamara+tech@proton.me
[pull]
  rebase = false
[push]
  autoSetupRemote = true
[commit]
  verbose = true
[submodule]
  recurse = true
[init]
  defaultBranch = main
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
